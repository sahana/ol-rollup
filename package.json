{
  "name": "ol-rollup",
  "version": "1.0.0",
  "description": "OpenLayers builds for Eden using Rollup",
  "scripts": {
    "start-ol6": "rollup --config ol6.config.js --watch",
    "build-ol6": "rollup --config ol6.config.js",
    "start-olgm": "rollup --config olgm.config.js --watch",
    "build-olgm": "rollup --config olgm.config.js",
    "start-dc_editor": "rollup --config dc_editor.config.js --watch",
    "build-dc_editor": "rollup --config dc_editor.config.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "ol": "^6.12.0",
    "olgm": "^1.7.1",
    "ol-layerswitcher": "^3.8.3"
  }
}
